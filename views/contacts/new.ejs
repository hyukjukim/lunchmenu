    <a class="btn btn-default" href="/contacts">Back</a>
   </div>
   <h2>New</h2>
   <form class="contact-form" action="/contacts" method="post">
    <div class="form-group">
     <label for="user_key">user_key</label>
     <input class="form-control" type="text" id="user_key" name="user_key" value="">
    </div>
    <div class="form-group">
     <label for="type">type</label>
     <input class="form-control" type="text" id="type" name="type" value="">
    </div>
    <div class="form-group">
     <label for="content">content</label>
     <input class="form-control" type="text" id="content" name="content" value="">
    </div>
    <div>
     <button type="submit" class="btn btn-default">Submit</button>
    </div>
   </form>
  </div>
 </body>
</html>
